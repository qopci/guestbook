<!DOCTYPE html>
<html>
<head>
    <title>Admin</title>
</head>
<body>
    <h1>All Entries</h1>
    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Jot Title</th>
                <th>Company</th>
                <th>LinkedIn URL</th>
                <th>Email</th>
                <th>How we met</th>
                <th>Other</th>
                <th>Message</th>
                <th>Mailing List</th>
                <th>Time Stamp</th>
            </tr>
        </thead>
        <tbody>
            <% details.forEach(form => { %>
                <tr>
                    <td><%= form.id %></td>
                    <td><%= form.fname %></td>
                    <td><%= form.lname %></td>
                    <td><%= form.jobtitle %></td>
                    <td><%= form.company %></td>
                    <td><%= form.linkedinurl %></td>
                    <td><%= form.email %></td>
                    <td><%= form.meeting %></td>
                    <td><%= form.other %></td>
                    <td><%= form.message %></td>
                    <td><%= form.mailinglist %></td>
                    <td><%= form.timestamp %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>